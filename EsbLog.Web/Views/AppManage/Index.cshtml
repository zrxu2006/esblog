@model IEnumerable<EsbLog.Domain.Platform.App>

@{
    ViewBag.Title = "应用管理";
}

<div class="page-header">
    <h2>应用管理</h2>
</div>

<div>
    <table class="table">
        <thead>
            <tr>
                <th>应用编号</th>
                <th>应用名称</th>
                <th>描述</th>
                <th>访问key</th>
            </tr>
        </thead>
        <tbody>
            
        @foreach (var app in Model)
        {
            <tr>
                <td>@app.Code</td>
                <td>@app.Name</td>
                <td>@app.Description</td>
                <td>@app.PublicKey</td>
            </tr>
        }
            
        </tbody>
    </table>
</div>